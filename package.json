{
  "name": "react-wireflow",
  "version": "0.3.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "bun@1.3.1",
  "sideEffects": [
    "dist/style.css",
    "public/themes/*.css"
  ],
  "license": "UNLICENSED",
  "files": [
    "dist",
    "public/themes"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./style.css": "./dist/style.css",
    "./themes/adobe.css": "./public/themes/adobe.css",
    "./themes/apple.css": "./public/themes/apple.css",
    "./themes/figma.css": "./public/themes/figma.css",
    "./themes/github-light.css": "./public/themes/github-light.css",
    "./themes/github.css": "./public/themes/github.css",
    "./themes/google.css": "./public/themes/google.css",
    "./themes/minecraft.css": "./public/themes/minecraft.css",
    "./themes/opal.css": "./public/themes/opal.css",
    "./themes/stellar.css": "./public/themes/stellar.css",
    "./themes/unity.css": "./public/themes/unity.css",
    "./themes/vscode.css": "./public/themes/vscode.css",
    "./themes/wargames.css": "./public/themes/wargames.css",
    "./themes/window98.css": "./public/themes/window98.css",
    "./themes/windows11.css": "./public/themes/windows11.css",
    "./themes/windowsxp.css": "./public/themes/windowsxp.css",
    "./themes/xcorp.css": "./public/themes/xcorp.css",
    "./i18n/en": {
      "types": "./dist/i18n/en.d.ts",
      "import": "./dist/i18n/en.js",
      "require": "./dist/i18n/en.cjs"
    },
    "./i18n/ja": {
      "types": "./dist/i18n/ja.d.ts",
      "import": "./dist/i18n/ja.js",
      "require": "./dist/i18n/ja.cjs"
    },
    "./i18n/zh": {
      "types": "./dist/i18n/zh.d.ts",
      "import": "./dist/i18n/zh.js",
      "require": "./dist/i18n/zh.cjs"
    },
    "./i18n/zh-cn": {
      "types": "./dist/i18n/zh-cn.d.ts",
      "import": "./dist/i18n/zh-cn.js",
      "require": "./dist/i18n/zh-cn.cjs"
    },
    "./i18n/ko": {
      "types": "./dist/i18n/ko.d.ts",
      "import": "./dist/i18n/ko.js",
      "require": "./dist/i18n/ko.cjs"
    },
    "./i18n/es": {
      "types": "./dist/i18n/es.d.ts",
      "import": "./dist/i18n/es.js",
      "require": "./dist/i18n/es.cjs"
    },
    "./i18n/fr": {
      "types": "./dist/i18n/fr.d.ts",
      "import": "./dist/i18n/fr.js",
      "require": "./dist/i18n/fr.cjs"
    },
    "./i18n/de": {
      "types": "./dist/i18n/de.d.ts",
      "import": "./dist/i18n/de.js",
      "require": "./dist/i18n/de.cjs"
    },
    "./inspector": {
      "types": "./dist/inspector/index.d.ts",
      "import": "./dist/inspector/index.js",
      "require": "./dist/inspector/index.cjs"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:examples": "vite build --config vite.config.examples.ts",
    "analyze": "vite build --mode analyze",
    "clean": "rimraf dist",
    "generate:theme-exports": "bun run scripts/generate-theme-exports.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test:cov": "vitest run --coverage",
    "test": "vitest --run "
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/three": "^0.180.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.1",
    "ajv": "^8.17.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.1.5",
    "jsdom": "^27.0.1",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "terser": "^5.44.0",
    "three": "^0.180.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vite-bundle-analyzer": "^1.2.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.1"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {},
  "description": "Composable React node editor canvas with configurable nodes, connections, and inspector UI.",
  "keywords": [
    "react",
    "node-editor",
    "workflow",
    "diagram",
    "graph",
    "wireflow",
    "react-wireflow"
  ]
}
