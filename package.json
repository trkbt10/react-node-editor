{
  "name": "node-editor",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "bun@1.3.0",
  "sideEffects": [
    "dist/style.css",
    "public/themes/*.css"
  ],
  "license": "UNLICENSED",
  "files": [
    "dist",
    "public/themes"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./style.css": "./dist/style.css",
    "./themes/*": {
      "import": "./public/themes/*.css",
      "require": "./public/themes/*.css",
      "default": "./public/themes/*.css"
    },
    "./i18n/en": {
      "types": "./dist/i18n/en.d.ts",
      "import": "./dist/i18n/en.js",
      "require": "./dist/i18n/en.cjs"
    },
    "./i18n/ja": {
      "types": "./dist/i18n/ja.d.ts",
      "import": "./dist/i18n/ja.js",
      "require": "./dist/i18n/ja.cjs"
    },
    "./i18n/zh": {
      "types": "./dist/i18n/zh.d.ts",
      "import": "./dist/i18n/zh.js",
      "require": "./dist/i18n/zh.cjs"
    },
    "./i18n/ko": {
      "types": "./dist/i18n/ko.d.ts",
      "import": "./dist/i18n/ko.js",
      "require": "./dist/i18n/ko.cjs"
    },
    "./i18n/es": {
      "types": "./dist/i18n/es.d.ts",
      "import": "./dist/i18n/es.js",
      "require": "./dist/i18n/es.cjs"
    },
    "./i18n/fr": {
      "types": "./dist/i18n/fr.d.ts",
      "import": "./dist/i18n/fr.js",
      "require": "./dist/i18n/fr.cjs"
    },
    "./i18n/de": {
      "types": "./dist/i18n/de.d.ts",
      "import": "./dist/i18n/de.js",
      "require": "./dist/i18n/de.cjs"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:examples": "vite build --config vite.config.examples.ts",
    "analyze": "vite build --mode analyze",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test:cov": "vitest run --coverage",
    "test": "vitest --run "
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/three": "^0.180.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "ajv": "^8.17.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.1.2",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^6.0.1",
    "three": "^0.180.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.9",
    "vite-bundle-analyzer": "^1.2.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {}
}
